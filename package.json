{
   "name": "tic-tac-toe-grow",
   "description": "A variant of Tic tac toe where the board grows.",
   "version": "0.388.0-alpha",
   "license": "Apache-2.0",
   "keywords": [
      "tic tac toe"
   ],
   "scripts": {
      "test": "npm run check-syntax && npm run test-js",
      "check-syntax": "npm run check-html-syntax && npm run check-js-syntax",
      "check-html-syntax": "npx html-validator-cli --file=game.html --verbose",
      "check-js-syntax": "npx standardx --verbose",
      "test-js": "jest"
   },
   "browser": "./game.html",
   "browserslist": [
      "> 5%",
      "last 2 versions",
      "not ie <= 10"
   ],
   "author": {
      "name": "Steven Nguyen",
      "email": "58114641+icecream17@users.noreply.github.com",
      "url": "https://icecream17.github.io/"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/icecream17/tic-tac-toe-grow-for-website.git"
   },
   "bugs": {
      "url": "https://github.com/icecream17/tic-tac-toe-grow-for-website/issues"
   },
   "devDependencies": {
      "@babel/core": "^7.14.0",
      "@babel/eslint-parser": "^7.13.14",
      "@babel/eslint-plugin": "^7.13.16",
      "@babel/plugin-syntax-class-properties": "^7.12.13",
      "@babel/plugin-proposal-class-properties": "^7.13.0",
      "@babel/preset-env": "^7.13.15",
      "@testing-library/jest-dom": "^5.12.0",
      "babel-jest": "^26.6.3",
      "core-js": "^3.11.0",
      "eslint": "^7.24.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jest": "^24.3.6",
      "eslint-plugin-jest-dom": "^3.8.1",
      "eslint-plugin-node": "^11.1.0",
      "html-validator-cli": "^7.0.1",
      "jest": "^27.0.6",
      "serve": "^12.0.0",
      "standardx": "^7.0.0"
   },
   "engines": {
      "npm": "^7.6",
      "node": ">=12.0.0"
   },
   "standardx": {
      "parser": "@babel/eslint-parser"
   },
   "jest": {
      "transform": {
         "\\.[jt]sx?$": "babel-jest"
      }
   }
}
